        "id": 0,
        "definition": {
          "type": "timeseries",
          "requests": [
            {
              "log_query": {
                "index": "*",
                "compute": {
                  "aggregation": "count"
                },
                "search": {
                  "query": "source:perimeterx $EventName $TrueIP $Continent $Country $Domain $ApplicationID"
                },
                "group_by": [
                  {
                    "facet": "@evt.name",
                    "limit": 10,
                    "sort": {
                      "aggregation": "count",
                      "order": "desc"
                    }
                  }
