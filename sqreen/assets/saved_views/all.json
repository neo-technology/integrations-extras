{
    "timerange": {
        "interval_ms": 86400000
    },
    "query": "source:sqreen",
    "name": "Sqreen | All logs",
    "visible_facets": [
        "@network.client.ip",
        "@sqreen.payload.automation_plugin_humanized_id",
        "@http.useragent_details.os.family",
        "@http.useragent_details.browser.family",
        "@http.url_details.path",
        "@sqreen.actor.identifier.username",
        "@userIdentity.session_name",
        "@sqreen.application.name",
        "@sqreen.payload.protection",
        "@http.useragent_details.device.family",
        "@error.kind",
        "@network.client.geoip.city.name",
        "@sqreen.actor.user.auth_id",
        "@sqreen.context.request.user_agent",
        "@eventSource",
        "@sqreen.actor.identifiers.email",
        "@sqreen.payload.severity",
        "@sqreen.application.environment",
        "@sqreen.context.request.path",
        "@http.method",
        "@usr.account_id",
        "@sqreen.actor.ip.geo.country_code",
        "@usr.email",
        "@network.client.geoip.subdivision.name",
        "@sqreen.payload.properties.success",
        "@userIdentity.arn",
        "@http.referer",
        "@sqreen.payload.attack",
        "@http.request_id",
        "@sqreen.signal_name",
        "@sqreen.actor.ip.address",
        "@sqreen.payload.name",
        "@sqreen.payload.ruleset",
        "@sqreen.context.response.blocked",
        "@evt.name",
        "@http.status_code",
        "@usr.name",
        "@userIdentity.assumed_role",
        "@usr.id",
        "@sqreen.payload.rule_id",
        "@aws.s3.bucket",
        "@network.client.geoip.country.name",
        "@duration"
    ],
    "page": "stream",
    "type": "logs",
    "options": {
        "show_date_column": true,
        "message_display": "inline",
        "show_message_column": true,
        "show_timeline": true,
        "columns": [
            "@env",
            "service",
            "@evt.name",
            "@sqreen.context.http.response.blocked"
        ]
    }
}