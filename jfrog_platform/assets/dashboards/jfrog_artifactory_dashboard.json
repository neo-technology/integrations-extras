{
    "author_name": "JFrog",
    "description": "This dashboard provides usage statistics overview of JFrog Artifactory logs. Included are also some high level views of audit, request and application logs. \n\nA detailed blog post can be found here: [Track JFrog Platform Performance with Datadog Analytics](https://jfrog.com/blog/track-jfrog-platform-performance-with-datadog-analytics/). \n\nSource code is also available in [Github](https://github.com/jfrog/log-analytics-datadog)",
    "layout_type": "free",
    "template_variables": [
        {
            "default": "*",
            "name": "ip_address",
            "prefix": "@ip_address"
        },
        {
            "default": "*",
            "name": "username",
            "prefix": "@user_name"
        },
        {
            "default": "*",
            "name": "repo",
            "prefix": "@repo"
        }
    ],
    "title": "JFrog Artifactory Dashboard",
    "widgets": [
        {
            "definition": {
                "background_color": "blue",
                "content": "Audit",
                "font_size": "18",
                "show_tick": false,
                "text_align": "center",
                "tick_edge": "bottom",
                "tick_pos": "50%",
                "type": "note"
            },
            "id": 0,
            "layout": {
                "height": 7,
                "width": 30,
                "x": 0,
                "y": 30
            }
        },
        {
            "definition": {
                "custom_links": [],
                "legend_size": "0",
                "requests": [
                    {
                        "display_type": "line",
                        "log_query": {
                            "compute": {
                                "aggregation": "count"
                            },
                            "group_by": [
                                {
                                    "facet": "@repo",
                                    "limit": 10,
                                    "sort": {
                                        "aggregation": "count",
                                        "order": "desc"
                                    }
                                }
                            ],
                            "index": "*",
                            "search": {
                                "query": "@log_source:jfrog.rt.artifactory.request @repo:*?*"
                            }
                        },
                        "style": {
                            "line_type": "solid",
                            "line_width": "normal",
                            "palette": "dog_classic"
                        }
                    }
                ],
                "show_legend": false,
                "title": "Accessed Repos",
                "title_align": "left",
                "title_size": "16",
                "type": "timeseries",
                "yaxis": {
                    "include_zero": true,
                    "label": "",
                    "max": "auto",
                    "min": "auto",
                    "scale": "linear"
                }
            },
            "id": 1,
            "layout": {
                "height": 14,
                "width": 37,
                "x": 32,
                "y": 71
            }
        },
        {
            "definition": {
                "custom_links": [],
